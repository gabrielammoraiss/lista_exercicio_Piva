   ],
   "source": [
    "letra = input(\"Digite uma letra: \").lower()\n",
    "if letra in (\"a\" \"e\" \"i\" \"o\" \"u\"):\n",
    "    print (f\"A letra digitada '{letra}' é uma letra vogal\")\n",
    "else:\n",
    "    print (f\"A letra digitada '{letra}' é uma letra consoante\")\n",
    "\n"
   ]
