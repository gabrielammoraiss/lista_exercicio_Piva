     "output_type": "stream",
     "text": [
      "Digite o 1º número :  8\n",
      "Digite o 2º número :  60\n",
      "Digite o 3º número :  50\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[60, 50, 8]\n"
     ]
