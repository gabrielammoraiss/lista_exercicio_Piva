   ],
   "source": [
    "lista = []\n",
    "for i in range(3):\n",
    "    n = int(input(f\"Digite o {i+1}º número : \"))  \n",
    "    lista.append(n)\n",
    "\n",
    "lista.sort(reverse=True)  \n",
    "print(lista)\n",
    "\n",
    "         "
   ]
